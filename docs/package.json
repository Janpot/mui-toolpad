{
  "name": "docs",
  "version": "0.1.18",
  "private": true,
  "author": "MUI Toolpad",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production next build --profile",
    "build:clean": "rimraf .next && yarn build",
    "build-sw": "node ./scripts/buildServiceWorker.js",
    "dev": "next dev --port 3003",
    "deploy": "git push upstream master:docs-v5",
    "export": "rimraf docs/export && next export --threads=3 -o export && yarn build-sw",
    "icons": "rimraf public/static/icons/* && node ./scripts/buildIcons.js",
    "typescript": "tsc -p tsconfig.json",
    "typescript:transpile": "cross-env BABEL_ENV=development babel-node --extensions \".tsx,.ts,.js\" scripts/formattedTSDemos",
    "typescript:transpile:dev": "cross-env BABEL_ENV=development babel-node --extensions \".tsx,.ts,.js\" scripts/formattedTSDemos --watch"
  },
  "dependencies": {
    "@babel/core": "7.22.5",
    "@babel/plugin-transform-object-assign": "7.22.5",
    "@babel/runtime-corejs2": "7.22.5",
    "@mui/icons-material": "5.13.7",
    "@mui/markdown": "github:Janpot/material-ui#head=yarn-2&workspace=@mui/markdown",
    "@mui/material": "5.13.7",
    "@mui/monorepo-docs": "github:Janpot/material-ui#head=yarn-2&workspace=docs",
    "@mui/utils": "5.13.7",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-optimize-clsx": "2.6.2",
    "babel-plugin-preval": "5.1.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "fg-loadcss": "3.1.0",
    "fs-extra": "11.1.1",
    "invariant": "2.2.4",
    "next": "13.4.7",
    "next-transpile-modules": "10.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rimraf": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@types/json-schema": "7.0.12",
    "@types/react-is": "18.2.1",
    "typescript": "5.1.6"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
